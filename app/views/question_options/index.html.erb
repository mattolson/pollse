<h1>Listing question_options</h1>

<table>
  <tr>
    <th>Question</th>
    <th>Response</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @question_options.each do |question_option| %>
  <tr>
    <td><%= question_option.question %></td>
    <td><%= question_option.response %></td>
    <td><%= link_to 'Show', question_option %></td>
    <td><%= link_to 'Edit', edit_question_option_path(question_option) %></td>
    <td><%= link_to 'Destroy', question_option, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Question option', new_question_option_path %>
